        <span class="nx">a</span><span class="p">.</span><span class="nx">substr</span><span class="p">(</span><span class="mi">0</span><span class="p">,</span> <span class="mi">4</span><span class="p">)</span>
      <span class="p">)</span>
    <span class="p">)</span>
      <span class="nx">mobileCheck</span> <span class="o">=</span> <span class="kc">true</span><span class="p">;</span>
  <span class="p">})(</span><span class="nb">navigator</span><span class="p">.</span><span class="nx">userAgent</span> <span class="o">||</span> <span class="nb">navigator</span><span class="p">.</span><span class="nx">vendor</span> <span class="o">||</span> <span class="nb">window</span><span class="p">.</span><span class="nx">opera</span><span class="p">);</span>
  <span class="k">return</span> <span class="nx">mobileCheck</span><span class="p">;</span>
